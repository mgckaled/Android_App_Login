<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.example.android_app_login">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/Theme.Android_App_Login">
    <activity
      android:name=".CadastroActivity"
      android:parentActivityName=".MainActivity" />
    <activity
      android:name=".Activity_uIWToggleButton"
      android:parentActivityName=".PrincipalActivity" />
    <activity
      android:name=".Activity_uIWSwitch"
      android:parentActivityName=".PrincipalActivity" />
    <activity
      android:name=".Activity_uIWSpinner"
      android:parentActivityName=".PrincipalActivity" />
    <activity
      android:name=".Activity_uIWRadioButton"
      android:parentActivityName=".PrincipalActivity" />
    <activity
      android:name=".Activity_uIWCheckBox"
      android:parentActivityName=".PrincipalActivity" />
    <activity
      android:name=".Activity_uIWButton"
      android:parentActivityName=".PrincipalActivity" />
    <activity
      android:name=".Activity_uIWEditText"
      android:parentActivityName=".PrincipalActivity" />
    <activity
      android:name=".Activity_uIWTextView"
      android:parentActivityName=".PrincipalActivity" />
    <activity
      android:name=".PrincipalActivity"
      android:parentActivityName=".MainActivity" />
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>

</manifest>